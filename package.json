  "private": true,
  "scripts": {
    "build": "NODE_ENV=production tsc -p tsconfig.json",
    "dev": "export NODE_ENV=development && nodemon ./src/index.ts",
    "start": "export NODE_ENV=development && ts-node ./src/index.ts",
    "test": "NODE_ENV=test NODE_TLS_REJECT_UNAUTHORIZED=0 jest",
    "test:migrate:up": "NODE_ENV=test npx sequelize-cli db:migrate",
    "migrate:up": "npx sequelize-cli db:migrate",
    "migrate:undo": "npx sequelize-cli db:migrate:undo",
    "migrate:undo:all": "npx sequelize-cli db:migrate:undo:all",
    "prod": "NODE_ENV=production node ./build/index.js"
  },
